order_name = order_item.name;\n\n          if (item_name === order_name) {\n            order_item.qty += item_qty;\n          }\n        }\n      }\n      database\n        .collection(\"orders\")\n        .add({\n          order: this.orderlist,\n        })\n        .then(() => {\n          location.reload();\n        });\n    },\n  },\n};\n</script>\n\n<style>\n#basketItems {\n  margin: 20px;\n}\n#actionBtn {\n  background-color: pink;\n  width: 100px;\n  border-radius: 5px;\n  font-size: 15px;\n}\n</style>\n"],"sourceRoot":"src/components"}]}