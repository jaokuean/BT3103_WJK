subtotal = 0;\n      for (let i = 0; i < this.basketList.length; i++) {\n        const curr_item = this.basketList[i];\n        console.log(\"-----------------------item accounted: \" + curr_item[0]);\n        const item_price = curr_item[2] * curr_item[1];\n        this.subtotal += item_price;\n      }\n    },\n  },\n  computed: {\n    calGrandTotal: function () {\n      return (this.subtotal * 1.07).toFixed(2);\n    },\n  },\n};\n</script>\n\n<style>\n#basketItems {\n  margin: 20px;\n}\n#calBtn {\n  background-color: pink;\n  border-radius: 5px;\n  font-size: 15px;\n}\n</style>\n"]}]}